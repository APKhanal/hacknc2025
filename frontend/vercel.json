{
  "version": 2,
    "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    },
    {
      "src": "api/backend/requirements.txt",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/backend/(.*)",
      "dest": "/api/backend/index.py"
    }
  ]
}
